<interface id="chemprop" version="release_26.1+chemprop">
    <block label="Workflow Description"><row><cell><label prefix="false"><content>&lt;div style="5px;padding:1px; border:2px;margin-left:8%;margin-right:8%;text-align:left"&gt;                    &lt;br&gt;&lt;strong&gt;CHEMPROP&lt;/strong&gt;                     &lt;hr style="margin-top:5px;margin-bottom:5px"&gt;                     ChemProp                     &lt;hr style="margin-top:5px;margin-bottom:5px"&gt;                     &lt;small&gt;Workflow version release_26.1+chemprop &lt;/small&gt;                     &lt;/div&gt;</content></label></cell></row></block><workflow-id>CHEMPROP</workflow-id>
    <workflow-label>CHEMPROP</workflow-label>
    <parameters>
        <parameter label="Description" name="desc">
            <validator maximum="1000" type="length" />
        </parameter>
        <parameter label="Email" name="email">
            <validator type="email" />
        </parameter>


        <parameter label="Enter GNPS Network Task ID (Required)" name="GNPS_FBMN_TASK">
            <default value="GNPS_FBMN_TASK" />
        </parameter>

        <parameter label="METADATA_COLUMN" name="METADATA_COLUMN">
            <default value="METADATA_COLUMN" />
        </parameter>

        <parameter label="TIME_SERIES" name="TIME_SERIES">
            <default value="TIME_SERIES" />
        </parameter>

        <parameter label="MIN_AREA_THRESHOLD" name="MIN_AREA_THRESHOLD">
            <default value="0" />
        </parameter>

        <parameter label="METADATA_FILTER_COLUMN" name="METADATA_FILTER_COLUMN">
            <default value="None" />
        </parameter>

        <parameter label="METADATA_FILTER_TERM" name="METADATA_FILTER_TERM">
            <default value="None" />
        </parameter>
    </parameters>

    
    <block label="Input Selection">
        <row>
            <cell>
                <label>
                    <content parameter="GNPS_FBMN_TASK" />
                </label>
            </cell>
            <cell>
                <input parameter="GNPS_FBMN_TASK" type="text">
                    <attribute name="size" value="30" />
                </input>
            </cell>
        </row>
        <row>
            <cell>
                <label>
                    <content parameter="METADATA_COLUMN" />
                </label>
            </cell>
            <cell>
                <input parameter="METADATA_COLUMN" type="text">
                    <attribute name="size" value="30" />
                </input>
            </cell>
        </row>
        <row>
            <cell>
                <label>
                    <content parameter="TIME_SERIES" />
                </label>
            </cell>
            <cell>
                <input parameter="TIME_SERIES" type="text">
                    <attribute name="size" value="30" />
                </input>
            </cell>
        </row>
        <row>
            <cell>
                <label>
                    <content parameter="MIN_AREA_THRESHOLD" />
                </label>
            </cell>
            <cell>
                <input parameter="MIN_AREA_THRESHOLD" type="text">
                    <attribute name="size" value="30" />
                </input>
            </cell>
        </row>
        <row>
            <cell>
                <label>
                    <content parameter="METADATA_FILTER_COLUMN" />
                </label>
            </cell>
            <cell>
                <input parameter="METADATA_FILTER_COLUMN" type="text">
                    <attribute name="size" value="30" />
                </input>
            </cell>
        </row>
        <row>
            <cell>
                <label>
                    <content parameter="METADATA_FILTER_TERM" />
                </label>
            </cell>
            <cell>
                <input parameter="METADATA_FILTER_TERM" type="text">
                    <attribute name="size" value="30" />
                </input>
            </cell>
        </row>
    </block>


</interface>